{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "LX119",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["perneelw", "Wim Perneel"],
    "createDate": "2020-10-08",
    "lastModifyDate": "2020-10-08",
    "importPlugin": {
      "plugin": "qlcplus_4.12.1",
      "date": "2020-10-08",
      "comment": "created by Q Light Controller Plus (version 4.12.2)"
    }
  },
  "physical": {
    "DMXconnector": "5-pin"
  },
  "availableChannels": {
    "Dimmer": {
      "defaultValue": 0,
      "capability": {
        "type": "Intensity"
      }
    },
    "Red": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Amber": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Amber"
      }
    },
    "White": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "White"
      }
    },
    "Indigo": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Indigo"
      }
    },
    "Strobe": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 10],
          "type": "NoFunction"
        },
        {
          "dmxRange": [11, 255],
          "type": "Effect",
          "effectName": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    }
  },
  "modes": [
    {
      "name": "8-channel",
      "shortName": "8ch",
      "channels": [
        "Dimmer",
        "Red",
        "Green",
        "Blue",
        "Amber",
        "White",
        "Indigo",
        "Strobe"
      ]
    },
    {
      "name": "6-channel",
      "shortName": "6ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Amber",
        "White",
        "Indigo"
      ]
    }
  ]
}
