{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "B117 Par Can 4in1 RGBW 18 LEDs",
  "shortName": "b117-par-can",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["Ken Harris"],
    "createDate": "2022-02-27",
    "lastModifyDate": "2022-02-27"
  },
  "comment": "This is an unbranded fixture bought online, but the appearance and user manual match the UKing B117 exactly.\n\nThe B117 webpage isn't even internally consistent, so I've done the best I could here.",
  "links": {
    "manual": [
      "https://www.uking-online.com/wp-content/uploads/2020/10/ZQ-B117.pdf"
    ],
    "productPage": [
      "https://www.uking-online.com/product/b117-par-can-4in1-rgbw-18-leds/"
    ]
  },
  "physical": {
    "dimensions": [225, 225, 115],
    "weight": 3.2,
    "power": 200,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "LED"
    }
  },
  "availableChannels": {
    "Dimmer": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "White": {
      "capability": {
        "type": "ColorIntensity",
        "color": "White"
      }
    },
    "Effect": {
      "capabilities": [
        {
          "dmxRange": [0, 50],
          "type": "NoFunction"
        },
        {
          "dmxRange": [51, 100],
          "type": "Effect",
          "effectName": "Different Colors Output",
          "helpWanted": "What is this effect? Is it different color presets at different DMX values?"
        },
        {
          "dmxRange": [101, 150],
          "type": "Effect",
          "effectPreset": "ColorJump"
        },
        {
          "dmxRange": [151, 200],
          "type": "Effect",
          "effectPreset": "ColorFade"
        },
        {
          "dmxRange": [201, 250],
          "type": "Effect",
          "effectName": "Colors Pulse Change"
        },
        {
          "dmxRange": [251, 255],
          "type": "Effect",
          "effectName": "Sound-Active",
          "soundControlled": true
        }
      ]
    },
    "Effect Speed": {
      "capability": {
        "type": "EffectSpeed",
        "speedStart": "slow",
        "speedEnd": "fast"
      }
    },
    "Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 0],
          "type": "ShutterStrobe",
          "shutterEffect": "Open"
        },
        {
          "dmxRange": [1, 255],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    }
  },
  "modes": [
    {
      "name": "8-channel",
      "shortName": "8ch",
      "channels": [
        "Dimmer",
        "Red",
        "Green",
        "Blue",
        "White",
        "Strobe",
        "Effect",
        "Effect Speed"
      ]
    },
    {
      "name": "4-channel",
      "shortName": "4ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "White"
      ]
    }
  ]
