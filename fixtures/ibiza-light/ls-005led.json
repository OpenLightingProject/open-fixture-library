        {
          "dmxRange": [1, 127],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast"
        },
        {
          "dmxRange": [128, 128],
          "type": "NoFunction"
        },
        {
          "dmxRange": [129, 255],
          "type": "Intensity"
        }
      ]
    },
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    }
  },
  "modes": [
    {
      "name": "4-channel",
      "shortName": "4ch",
      "channels": [
        "Dimmer",
        "Red",
        "Green",
        "Blue"
      ]
    }
  ]
