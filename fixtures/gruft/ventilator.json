    "Red": {
      "highlightValue": 255,
      "precedence": "HTP",
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "highlightValue": 255,
      "precedence": "HTP",
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "highlightValue": 255,
      "precedence": "HTP",
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
