        {
          "dmxRange": [120, 139],
          "type": "Rotation",
          "speed": "stop",
          "menuClick": "center"
        },
        {
          "dmxRange": [140, 255],
          "type": "Rotation",
          "speedStart": "slow CW",
          "speedEnd": "fast CW",
          "menuClick": "center"
        }
      ]
