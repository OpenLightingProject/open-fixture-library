          "switchChannels": {
            "Show Speed / Sound Sensitivity": "Show Speed"
          }
        },
        {
          "dmxRange": [8, 15],
          "type": "ColorPreset",
          "comment": "Pattern red",
          "colors": ["#ff0000"],
          "switchChannels": {
            "Show Speed / Sound Sensitivity": "Show Speed"
          }
        },
        {
          "dmxRange": [16, 23],
          "type": "ColorPreset",
          "comment": "Pattern yellow",
          "colors": ["#ffff00"],
          "switchChannels": {
            "Show Speed / Sound Sensitivity": "Show Speed"
          }
        },
        {
          "dmxRange": [24, 31],
          "type": "ColorPreset",
          "comment": "Pattern green",
          "colors": ["#00ff00"],
