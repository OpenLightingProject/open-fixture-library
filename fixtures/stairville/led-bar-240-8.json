    "Blue $pixelKey": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    }
  },
  "modes": [
    {
      "name": "2-channel",
      "shortName": "2ch",
      "channels": [
        "Programs",
        "Show Speed / Sound Sensitivity"
      ]
    },
    {
      "name": "3-channel",
      "shortName": "3ch",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master"
      ]
    },
    {
      "name": "5-channel",
      "shortName": "5ch",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master",
        "Dimmer",
        "Strobe"
      ]
    },
    {
      "name": "24-channel",
      "shortName": "24ch",
      "channels": [
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelABC",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        }
      ]
    }
  ]
