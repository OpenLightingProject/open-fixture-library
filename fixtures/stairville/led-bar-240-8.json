          "switchChannels": {
            "Show Speed / Sound Sensitivity": "Show Speed"
          }
        },
        {
          "dmxRange": [56, 63],
          "type": "ColorPreset",
          "comment": "Pattern white",
          "colors": ["#ffffff"],
          "switchChannels": {
            "Show Speed / Sound Sensitivity": "Show Speed"
          }
        },
        {
          "dmxRange": [64, 231],
          "type": "Effect",
