{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "CLB5 2P RGB WW Compact LED Bar",
  "categories": ["Stand", "Color Changer"],
  "meta": {
    "authors": ["Murph"],
    "createDate": "2021-10-27",
    "lastModifyDate": "2021-10-27"
  },
  "links": {
    "manual": [
      "https://images.static-thomann.de/pics/atg/atgdata/document/manual/c_448711_v4_en_online.pdf"
    ],
    "productPage": [
      "https://www.thomannmusic.com/stairville_clb5_2p_rgb_ww_compact_led_bar.htm"
    ]
  },
  "physical": {
    "dimensions": [473, 253, 67],
    "weight": 305,
    "power": 60,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "LED"
    }
  },
  "matrix": {
    "pixelKeys": [
      [
        ["Spot 1", "Spot 2"]
      ]
    ],
    "pixelGroups": {
      "Master": "all"
    }
  },
  "templateChannels": {
    "Red $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Warm White $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Warm White"
      }
    }
  },
  "availableChannels": {
    "Dimmer": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 4],
          "type": "ShutterStrobe",
          "shutterEffect": "Open"
        },
        {
          "dmxRange": [5, 255],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "0Hz",
          "speedEnd": "20Hz"
        }
      ]
    }
  },
  "modes": [
    {
      "name": "4-channel",
      "shortName": "4ch",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master",
        "Warm White Master"
      ]
    },
    {
      "name": "8-channel",
      "shortName": "8ch",
      "channels": [
        "Red Spot 1",
        "Green Spot 1",
        "Blue Spot 1",
        "Warm White Spot 1",
        "Red Spot 2",
        "Green Spot 2",
        "Blue Spot 2",
        "Warm White Spot 2"
      ]
    },
    {
      "name": "10-channel",
      "shortName": "10ch",
      "channels": [
        "Dimmer",
        "Red Spot 1",
        "Green Spot 1",
        "Blue Spot 1",
        "Warm White Spot 1",
        "Red Spot 2",
        "Green Spot 2",
        "Blue Spot 2",
        "Warm White Spot 2",
        "Strobe"
      ]
    }
  ]
}
