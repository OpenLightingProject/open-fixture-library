      "shortName": "2ch 2",
      "channels": [
        "Intensity Warm White",
        "Intensity Cold White"
      ]
    },
    {
      "name": "3-channel",
      "shortName": "3ch",
      "channels": [
        "Dimmer",
        "Strobe",
        "Color Temperature"
      ]
    },
    {
      "name": "4-channel",
      "shortName": "4ch",
      "channels": [
        "Dimmer",
        "Strobe",
        "Intensity Warm White",
        "Intensity Cold White"
