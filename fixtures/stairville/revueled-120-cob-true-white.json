{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "RevueLED 120 COB True White",
  "categories": ["Dimmer"],
  "meta": {
    "authors": ["Hannes Rüger", "OFL-Exported"],
    "createDate": "2026-01-14",
    "lastModifyDate": "2026-01-14",
    "importPlugin": {
      "plugin": "qlcplus_4.12.1",
      "date": "2026-01-14",
      "comment": "created by OFL – https://open-fixture-library.org/stairville/revueled-120-cob-true-white (version 1.3.1)"
    }
  },
  "physical": {
    "dimensions": [440, 250, 160],
    "weight": 5.5,
    "power": 120,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "1× 120W COB CW/WW LED",
      "lumens": 7000
