      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 5],
          "type": "Generic",
          "comment": "Color temperature adjustment (2800K…7800K) follows dimmer value"
        },
        {
          "dmxRange": [6, 255],
          "type": "Generic",
          "comment": "Color temperature 2800…7800K"
        }
      ]
    },
    "Intensity Warm White": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Warm White"
      }
    },
    "Intensity Cold White": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Cold White"
      }
    },
    "Strobe": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 5],
          "type": "ShutterStrobe",
          "shutterEffect": "Open",
          "comment": "Shutter open"
        },
        {
          "dmxRange": [6, 255],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "1Hz",
          "speedEnd": "30Hz",
          "comment": "Strobe 1…30Hz"
        }
      ]
    }
