{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "Alien S",
  "categories": ["Pixel Bar", "Color Changer"],
  "meta": {
    "authors": ["Yéred Zabdiel"],
    "createDate": "2021-11-08",
    "lastModifyDate": "2021-11-08"
  },
  "comment": "LED bar",
  "links": {
    "manual": [
      "http://alienpro.com.mx/wp-content/uploads/Manual-Bar-Alien-S.pdf"
    ],
    "video": [
      "https://www.youtube.com/watch?v=iuyEvc5mtj4"
    ]
  },
  "physical": {
    "dimensions": [1070, 65, 88],
    "weight": 2.6,
    "power": 30,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "240× 10mm LED (96 red, 72 green, 72 blue)"
    },
    "lens": {
      "degreesMinMax": [30, 30]
    }
  },
  "matrix": {
    "pixelCount": [8, 1, 1],
    "pixelGroups": {
      "Master": "all",
      "1/4": ["1", "2"],
      "2/4": ["3", "4"],
      "3/4": ["5", "6"],
      "4/4": ["7", "8"]
    }
  },
  "templateChannels": {
    "Red $pixelKey": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green $pixelKey": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue $pixelKey": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    }
  },
  "availableChannels": {
    "Master Dimmer": {
      "defaultValue": 0,
      "capability": {
        "type": "Intensity"
      }
    },
    "Flash Speed": {
      "capability": {
        "type": "Intensity"
      }
    }
  },
  "modes": [
    {
      "name": "3-channel d-P1",
      "shortName": "d-P1",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master"
      ]
    },
    {
      "name": "4-channel d-P2",
      "shortName": "d-P2",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master",
        "Master Dimmer"
      ]
    },
    {
      "name": "14-channel d-P3",
      "shortName": "d-P3",
      "channels": [
        {
          "insert": "matrixChannels",
          "repeatFor": [
            "1/4",
            "2/4",
            "3/4",
            "4/4"
          ],
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        },
        "Flash Speed",
        "Master Dimmer"
      ]
    },
    {
      "name": "26-channel d-P4",
      "shortName": "d-P4",
      "channels": [
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelXYZ",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        },
        "Flash Speed",
        "Master Dimmer"
      ]
    }
  ]
}
