{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "LED SLS 183-10",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["carlos"],
    "createDate": "2024-12-04",
    "lastModifyDate": "2024-12-04"
  },
  "links": {
    "manual": [
      "https://www.steinigke.de/download/51915310-Anleitung-90908-1.200-eurolite-led-sls-183-rgb-10mm-floor-de_en.pdf"
    ],
    "productPage": [
      "https://www.steinigke.de/de/mpn51915310-eurolite-led-sls-183-rgb-10mm-floor.html"
    ]
  },
  "physical": {
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "LED"
    }
  },
  "availableChannels": {
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Dimmer": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 0],
          "type": "NoFunction"
        },
        {
          "dmxRange": [1, 5],
          "type": "SoundSensitivity",
          "soundSensitivityStart": "low",
          "soundSensitivityEnd": "high"
        },
        {
          "dmxRange": [6, 10],
          "type": "NoFunction"
        },
        {
          "dmxRange": [11, 255],
          "type": "StrobeSpeed",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    }
  },
  "modes": [
    {
      "name": "Standard",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Dimmer",
        "Strobe"
      ]
    }
  ]
}
