{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "LED H2O",
  "shortName": "ELH2O",
  "categories": ["Effect", "Color Changer"],
  "meta": {
    "authors": ["Felix Edelmann"],
    "createDate": "2018-11-08",
    "lastModifyDate": "2018-11-08"
  },
  "comment": "Don't confuse this fixture with the Eurolite LED H2O TCL, which is only controllable with an IR remote.",
  "links": {
    "manual": [
      "https://media.steinigke.de/documents_t/700-MANUAL-1.20-en-de_00079684.pdf"
    ],
    "productPage": [
      "https://www.steinigke.de/en/mpn51918700-eurolite-led-h2o-water-effect.html"
    ],
    "video": [
      "https://www.youtube.com/watch?v=idoxSYB6oHk"
    ]
  },
  "physical": {
    "dimensions": [165, 170, 135],
    "weight": 0.8,
    "power": 26,
    "DMXconnector": "3.5mm stereo jack",
    "bulb": {
      "type": "9W TCL LED"
    },
    "lens": {
      "degreesMinMax": [35, 35]
    }
  },
  "availableChannels": {
    "Red": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Dimmer": {
      "defaultValue": 0,
      "capability": {
        "type": "Intensity"
      }
    },
    "Shutter": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 0],
          "type": "ShutterStrobe",
          "shutterEffect": "Open"
        },
        {
          "dmxRange": [1, 5],
          "type": "ShutterStrobe",
          "shutterEffect": "Closed"
        },
        {
          "dmxRange": [6, 10],
          "type": "ShutterStrobe",
          "shutterEffect": "Open"
        },
        {
          "dmxRange": [11, 255],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    }
  },
  "modes": [
    {
      "name": "5-channel",
      "shortName": "5ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Dimmer",
        "Shutter"
      ]
    }
  ]
}
