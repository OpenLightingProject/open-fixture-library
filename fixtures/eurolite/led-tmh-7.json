{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "LED TMH-7",
  "categories": ["Moving Head", "Color Changer"],
  "meta": {
    "authors": ["Flo Edelmann"],
    "createDate": "2017-09-07",
    "lastModifyDate": "2018-09-04"
  },
  "links": {
    "manual": [
      "https://media.steinigke.de/documents_t/51785973-MANUAL-2.10-en-de_00093407.pdf"
    ],
    "video": [
      "https://www.youtube.com/watch?v=XvNNbRJAUKE"
    ]
  },
  "physical": {
    "dimensions": [175, 245, 175],
    "weight": 3,
    "power": 75,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "18x 3W tricolor LED"
    },
    "lens": {
      "degreesMinMax": [25, 25]
    }
  },
  "availableChannels": {
    "Pan": {
      "fineChannelAliases": ["Pan fine"],
      "dmxValueResolution": "8bit",
      "defaultValue": 128,
      "capability": {
        "type": "Pan",
        "angleStart": "0deg",
        "angleEnd": "540deg"
      }
    },
    "Tilt": {
      "fineChannelAliases": ["Tilt fine"],
      "dmxValueResolution": "8bit",
      "defaultValue": 128,
      "capability": {
        "type": "Tilt",
        "angleStart": "0deg",
        "angleEnd": "180deg"
      }
    },
    "Pan / Tilt Speed": {
      "capability": {
        "type": "PanTiltSpeed",
        "speedStart": "fast",
        "speedEnd": "slow"
      }
    },
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Color Macros": {
      "capabilities": [
        {
          "dmxRange": [0, 40],
          "type": "NoFunction"
        },
        {
          "dmxRange": [41, 80],
          "type": "ColorPreset",
          "comment": "Macro 1",
          "helpWanted": "What colors can be selected in this channel?"
        },
        {
          "dmxRange": [81, 120],
          "type": "ColorPreset",
          "comment": "Macro 2"
        },
        {
          "dmxRange": [121, 140],
          "type": "ColorPreset",
          "comment": "Macro 3"
        },
        {
          "dmxRange": [141, 160],
          "type": "ColorPreset",
          "comment": "Macro 4"
        },
        {
          "dmxRange": [161, 200],
          "type": "ColorPreset",
          "comment": "Macro 5"
        },
        {
          "dmxRange": [201, 240],
          "type": "ColorPreset",
          "comment": "Macro 6"
        },
        {
          "dmxRange": [241, 255],
          "type": "NoFunction"
        }
      ]
    },
    "Shutter": {
      "defaultValue": 255,
      "capabilities": [
        {
          "dmxRange": [0, 10],
          "type": "ShutterStrobe",
          "shutterEffect": "Closed"
        },
        {
          "dmxRange": [11, 20],
          "type": "NoFunction",
          "menuClick": "hidden"
        },
        {
          "dmxRange": [21, 30],
          "type": "Maintenance",
          "comment": "Reset"
        },
        {
          "dmxRange": [31, 200],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast",
          "menuClick": "center"
        },
        {
          "dmxRange": [201, 250],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast",
          "randomTiming": true,
          "menuClick": "center"
        },
        {
          "dmxRange": [251, 255],
          "type": "NoFunction"
        }
      ]
    },
    "Dimmer Intensity": {
      "capability": {
        "type": "Intensity"
      }
    }
  },
  "modes": [
    {
      "name": "11-channel",
      "shortName": "11ch",
      "channels": [
        "Pan",
        "Tilt",
        "Pan / Tilt Speed",
        "Red",
        "Green",
        "Blue",
        "Color Macros",
        "Shutter",
        "Dimmer Intensity",
        "Pan fine",
        "Tilt fine"
      ]
    }
  ]
}
