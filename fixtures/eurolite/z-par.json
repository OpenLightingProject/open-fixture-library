    "Zoom": {
      "defaultValue": 0,
      "capability": {
        "type": "Zoom",
        "angleStart": "narrow",
        "angleEnd": "wide"
      }
    },
    "Effect Speed": {
      "defaultValue": 0,
      "capability": {
        "type": "EffectSpeed",
        "speedStart": "fast",
        "speedEnd": "slow",
        "comment": "ZoomSpeed"
      }
    },
    "Dimmer": {
      "defaultValue": 0,
      "capability": {
        "type": "Intensity"
      }
    },
    "Strobe": {
      "capability": {
        "type": "StrobeSpeed",
        "speedStart": "0Hz",
        "speedEnd": "40Hz"
      }
    },
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Cold White": {
      "capability": {
        "type": "ColorIntensity",
        "color": "White"
      }
    },
    "Color Presets": {
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 119],
          "type": "Speed",
          "speedStart": "slow",
          "speedEnd": "fast"
        },
        {
          "dmxRange": [120, 255],
          "type": "SoundSensitivity",
          "soundSensitivityStart": "low",
          "soundSensitivityEnd": "high"
        }
      ]
    }
  },
  "modes": [
    {
      "name": "Eurolite Z-PAR",
      "shortName": "9ch",
      "channels": [
        "Zoom",
        "Effect Speed",
        "Dimmer",
        "Strobe",
        "Red",
        "Green",
        "Blue",
        "Cold White",
        "Color Presets"
