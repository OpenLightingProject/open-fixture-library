    "Blue": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Warm White": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Warm White"
      }
