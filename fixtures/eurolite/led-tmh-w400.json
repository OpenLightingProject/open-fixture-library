    "Tilt": {
      "fineChannelAliases": ["Tilt fine"],
      "defaultValue": 0,
      "capability": {
        "type": "Tilt",
        "angleStart": "0deg",
        "angleEnd": "230deg"
      }
    },
    "Pan/Tilt speed": {
      "defaultValue": 0,
      "capability": {
        "type": "PanTiltSpeed",
        "speedStart": "fast",
        "speedEnd": "slow"
      }
    },
    "Zoom": {
      "defaultValue": 0,
      "capability": {
        "type": "Zoom",
        "angleStart": "wide",
        "angleEnd": "narrow"
      }
    },
    "Dimmer": {
      "defaultValue": 0,
      "capability": {
        "type": "Intensity"
      }
    },
    "Strobe": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 3],
          "type": "NoFunction"
        },
        {
          "dmxRange": [4, 127],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast",
          "comment": "Strobe effect with increasing speed"
        },
        {
          "dmxRange": [128, 191],
          "type": "ShutterStrobe",
          "shutterEffect": "Pulse",
          "speedStart": "slow",
          "speedEnd": "fast",
          "comment": "Pulse effect with increasing speed"
        },
        {
          "dmxRange": [192, 251],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast",
          "randomTiming": true,
          "comment": "Random strobe effect with increasing speed"
        },
        {
          "dmxRange": [252, 255],
          "type": "NoFunction"
        }
      ]
    },
    "Red": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
