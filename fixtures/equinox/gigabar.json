        "speedEnd": "fast"
      }
    },
    "Sound Sensitivity": {
      "capability": {
        "type": "SoundSensitivity",
        "soundSensitivityStart": "low",
        "soundSensitivityEnd": "high"
      }
    },
    "Flash": {
      "capability": {
        "type": "ShutterStrobe",
