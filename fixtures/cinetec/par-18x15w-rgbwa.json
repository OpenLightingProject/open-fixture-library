{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "PAR 18x15W RGBWA",
  "shortName": "SFLP17A",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["Arthur"],
    "createDate": "2019-02-18",
    "lastModifyDate": "2019-02-18"
  },
  "helpWanted": "Please provide a manual if possible. Why are there two color macro channels? Is the 5-channel mode correct?",
  "links": {
    "other": [
      "https://www.9pboutik.com/boutique/lumieres/par-led/par-18x15-rgbwa-pro.html"
    ]
  },
  "physical": {
    "weight": 6.6,
    "power": 270,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "18x15W RGBWA LEDs"
    },
    "lens": {
      "degreesMinMax": [15, 45]
    }
  },
  "availableChannels": {
    "Intensity": {
      "defaultValue": 0,
      "capability": {
        "type": "Intensity"
      }
    },
    "Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 10],
          "type": "NoFunction"
        },
        {
          "dmxRange": [11, 255],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "1Hz",
          "speedEnd": "25Hz"
        }
      ]
    },
    "Color Macros": {
      "defaultValue": 10,
      "capability": {
        "type": "ColorPreset",
        "comment": "Color Mix",
        "helpWanted": "At which DMX values can which color be selected?"
      }
    },
    "Color Macros 2": {
      "capability": {
        "type": "ColorPreset",
        "comment": "Color Mix 2",
        "helpWanted": "At which DMX values can which color be selected?"
      }
    },
    "Red": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "White": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "White"
      }
    },
    "Amber": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Amber"
      }
    }
  },
  "modes": [
    {
      "name": "9-channel",
      "shortName": "9ch",
      "channels": [
        "Intensity",
        "Strobe",
        "Color Macros",
        "Color Macros 2",
        "Red",
        "Green",
        "Blue",
        "White",
        "Amber"
      ]
    },
    {
      "name": "5-channel",
      "shortName": "5ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "White",
        "Amber"
      ]
    }
  ]
}
