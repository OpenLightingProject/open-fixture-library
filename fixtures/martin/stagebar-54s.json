{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "Stagebar 54S",
  "categories": ["Pixel Bar", "Color Changer"],
  "meta": {
    "authors": ["Andy Melsom"],
    "createDate": "2019-10-06",
    "lastModifyDate": "2019-10-06"
  },
  "comment": "Uses RJ-45 DMX connector",
  "links": {
    "manual": [
      "https://www.martin.com/files/files/productdocuments/11_MANUALS/999/UM_Stagebar54_EN_C.pdf"
    ],
    "productPage": [
      "https://www.martin.com/en/products/stagebar-54"
    ],
    "video": [
      "https://www.youtube.com/watch?v=kxgCK4c4baU"
    ]
  },
  "physical": {
    "dimensions": [420, 105, 190],
    "weight": 5.5,
    "power": 230,
    "bulb": {
      "type": "Luxeon K2 LEDs",
      "lumens": 650
    },
    "lens": {
      "degreesMinMax": [29, 29]
    },
    "matrixPixels": {
      "dimensions": [70, 70, 0],
      "spacing": [0, 0, 0]
    }
  },
  "matrix": {
    "pixelCount": [6, 1, 1],
    "pixelGroups": {
      "Master": "all",
      "1/2": ["1", "2", "3"],
      "2/2": ["4", "5", "6"],
      "1/3": ["1", "2"],
      "2/3": ["3", "4"],
      "3/3": ["5", "6"]
    }
  },
  "templateChannels": {
    "Red $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Amber $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Amber"
      }
    },
    "White $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "White"
      }
    },
    "Hue $pixelKey": {
      "capability": {
        "type": "Generic"
      }
    },
    "Saturation $pixelKey": {
      "capability": {
        "type": "Generic"
      }
    },
    "Intensity $pixelKey": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Color Temperature $pixelKey": {
      "defaultValue": 191,
      "capability": {
        "type": "ColorTemperature",
        "colorTemperatureStart": "2000K",
        "colorTemperatureEnd": "10000K"
      }
    }
  },
  "modes": [
    {
      "name": "RGB All",
      "channels": [
        {
          "insert": "matrixChannels",
          "repeatFor": ["Master"],
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        }
      ]
    },
    {
      "name": "RGBAW All",
      "channels": [
        {
          "insert": "matrixChannels",
          "repeatFor": ["Master"],
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey",
            "Amber $pixelKey",
            "White $pixelKey"
          ]
        }
      ]
    },
    {
      "name": "HSI All",
