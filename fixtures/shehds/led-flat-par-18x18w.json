  "physical": {
    "dimensions": [200, 115, 265],
    "weight": 4,
    "power": 324,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "18x18W LEDs"
    }
  },
  "availableChannels": {
    "Dimmer": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "White": {
      "capability": {
        "type": "ColorIntensity",
        "color": "White"
      }
    },
    "Amber": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Amber"
      }
    },
    "UV": {
      "capability": {
        "type": "ColorIntensity",
        "color": "UV"
      }
    },
    "Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 0],
          "type": "ShutterStrobe",
          "shutterEffect": "Open"
        },
        {
          "dmxRange": [1, 255],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "0Hz",
          "speedEnd": "10Hz"
        }
      ]
    },
    "Program": {
      "capabilities": [
        {
          "dmxRange": [0, 50],
          "type": "NoFunction"
        },
        {
          "dmxRange": [51, 100],
          "type": "Effect",
          "effectPreset": "ColorJump"
        },
        {
          "dmxRange": [101, 150],
          "type": "Effect",
          "effectPreset": "ColorFade"
        },
        {
          "dmxRange": [151, 200],
          "type": "Effect",
          "effectName": "Color Pulse"
        },
        {
          "dmxRange": [201, 255],
          "type": "Effect",
          "effectName": "Sound Control",
          "soundControlled": true
        }
      ]
    },
    "Function Speed": {
      "capability": {
        "type": "EffectSpeed",
        "speedStart": "slow",
        "speedEnd": "slow"
      }
    }
  },
  "modes": [
    {
      "name": "10-channel",
