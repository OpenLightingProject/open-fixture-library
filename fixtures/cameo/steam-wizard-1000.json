{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "Steam Wizard 1000",
  "shortName": "CLSW1000",
  "categories": ["Smoke", "Color Changer"],
  "meta": {
    "authors": ["Felix Edelmann"],
    "createDate": "2018-11-28",
    "lastModifyDate": "2018-11-28"
  },
  "links": {
    "manual": [
      "https://adamhall.s3.amazonaws.com/media/MARKEN/CAMEO/CLSW1000/CLSW1000_Cameo_Bedienungsanleitung_EN_DE_FR_ES_PL_IT.pdf"
    ],
    "video": [
      "https://www.youtube.com/watch?v=7itkJUl1CnM"
    ]
  },
  "physical": {
    "dimensions": [405, 200, 310],
    "weight": 7.5,
    "power": 800,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "9x 3W LEDs (3x red, 3x green, 3x blue)"
    }
  },
  "availableChannels": {
    "Fog": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 0],
          "type": "Fog",
          "fogType": "Fog",
          "fogOutput": "off"
        },
        {
          "dmxRange": [1, 255],
          "type": "Fog",
          "fogType": "Fog",
          "fogOutputStart": "1m^3/min",
          "fogOutputEnd": "150m^3/min"
        }
      ]
    },
    "Dimmer": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 4],
          "type": "Intensity",
          "brightness": "off"
        },
        {
          "dmxRange": [5, 255],
          "type": "Intensity",
          "brightnessStart": "dark",
          "brightnessEnd": "bright"
        }
      ]
    },
    "Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 5],
          "type": "NoFunction"
        },
        {
          "dmxRange": [6, 255],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "1Hz",
          "speedEnd": "30Hz"
        }
      ]
    },
    "Color": {
      "capabilities": [
        {
          "dmxRange": [0, 5],
          "type": "NoFunction"
        },
        {
          "dmxRange": [6, 13],
