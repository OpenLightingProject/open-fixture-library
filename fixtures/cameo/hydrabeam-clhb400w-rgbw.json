  "templateChannels": {
    "Pan $pixelKey": {
      "fineChannelAliases": ["Pan $pixelKey fine"],
      "defaultValue": "50%",
      "capability": {
        "type": "Pan",
        "angleStart": "0deg",
        "angleEnd": "540deg"
      }
    },
    "Tilt $pixelKey": {
      "fineChannelAliases": ["Tilt $pixelKey fine"],
      "defaultValue": "50%",
      "capability": {
        "type": "Tilt",
        "angleStart": "0deg",
        "angleEnd": "270deg"
      }
    },
    "Dimmer $pixelKey": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Head Speed $pixelKey simple": {
      "capabilities": [
        {
          "dmxRange": [0, 10],
          "type": "PanTiltSpeed",
          "speed": "fast"
        },
        {
          "dmxRange": [11, 255],
          "type": "PanTiltSpeed",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    },
    "Head Speed $pixelKey": {
      "capabilities": [
        {
          "dmxRange": [0, 10],
          "type": "Maintenance",
          "comment": "Use Head Speed 1"
