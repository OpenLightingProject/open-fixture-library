{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "led",
  "shortName": "led",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["shaik"],
    "createDate": "2024-08-02",
    "lastModifyDate": "2024-08-02"
  },
  "links": {
    "productPage": [
      "https://open-fixture-library.org/fixture-editor"
    ]
  },
  "availableChannels": {
    "Dimmer": {
      "defaultValue": 1,
      "capability": {
        "type": "ShutterStrobe",
        "shutterEffect": "Open"
      }
    },
    "Red": {
      "defaultValue": 2,
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "defaultValue": 3,
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "defaultValue": 4,
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Shutter / Strobe": {
      "defaultValue": 5,
      "capability": {
        "type": "ShutterStrobe",
        "shutterEffect": "Strobe"
      }
    },
    "Effect Speed": {
      "defaultValue": 6,
      "capability": {
        "type": "EffectSpeed",
        "speed": "slow"
      }
    }
  },
  "modes": [
    {
      "name": "standar",
      "channels": [
        "Dimmer",
        "Red",
        "Green",
        "Blue",
        "Shutter / Strobe",
        "Effect Speed"
      ]
    }
  ]
}
