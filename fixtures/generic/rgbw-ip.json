    "importPlugin": {
      "plugin": "qlcplus_4.12.1",
      "date": "2022-06-18",
      "comment": "created by Q Light Controller Plus (version 4.12.4a)"
    }
  },
  "physical": {
    "power": 180,
    "DMXconnector": "3-pin",
