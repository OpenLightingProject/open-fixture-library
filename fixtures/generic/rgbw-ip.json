    "Speed": {
      "defaultValue": 0,
      "capability": {
        "type": "Speed",
        "speedStart": "slow",
        "speedEnd": "fast",
        "helpWanted": "Are the automatically added speed values correct?"
      }
    }
  },
  "modes": [
    {
      "name": "1",
      "channels": [
        "Dimmer",
        "Red",
        "Green",
        "Blue",
        "White",
        "Function choose",
        "Speed"
      ]
    }
