{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "CW/WW Fader",
  "categories": ["Dimmer", "Color Changer"],
  "meta": {
    "authors": ["sonarkon"],
    "createDate": "2019-06-23",
    "lastModifyDate": "2019-06-23"
  },
  "availableChannels": {
    "Warm White": {
      "fineChannelAliases": ["Warm White fine", "Warm White fine^2"],
      "capability": {
        "type": "ColorIntensity",
        "color": "Warm White"
      }
    },
    "Cold White": {
      "fineChannelAliases": ["Cold White fine", "Cold White fine^2"],
      "capability": {
        "type": "ColorIntensity",
        "color": "Cold White"
      }
    }
  },
  "modes": [
    {
      "name": "8 bit W/C",
      "shortName": "8bit-wc",
      "channels": [
        "Warm White",
        "Cold White"
      ]
    },
    {
      "name": "8 bit C/W",
      "shortName": "8bit-cw",
      "channels": [
        "Cold White",
        "Warm White"
      ]
    },
    {
      "name": "16 bit W/C",
      "shortName": "16bit-wc",
      "channels": [
        "Warm White",
        "Warm White fine",
        "Cold White",
        "Cold White fine"
      ]
    },
    {
      "name": "16 bit C/W",
      "shortName": "16bit-cw",
      "channels": [
        "Cold White",
        "Cold White fine",
        "Warm White",
        "Warm White fine"
      ]
    },
    {
      "name": "24 bit W/C",
      "shortName": "24bit-wc",
      "channels": [
        "Warm White",
        "Warm White fine",
        "Warm White fine^2",
        "Cold White",
        "Cold White fine",
        "Cold White fine^2"
      ]
    },
    {
      "name": "24 bit C/W",
      "shortName": "24bit-cw",
      "channels": [
        "Cold White",
        "Cold White fine",
        "Cold White fine^2",
        "Warm White",
        "Warm White fine",
        "Warm White fine^2"
      ]
    }
  ]
}
