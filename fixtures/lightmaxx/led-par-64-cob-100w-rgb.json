{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "LED PAR 64 COB 100W RGB",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["Mindful", "Flo Edelmann"],
    "createDate": "2026-01-28",
    "lastModifyDate": "2026-01-28"
  },
  "links": {
    "manual": [
      "http://ls-systems.be/matos/lumiere/spot/LIGHTMAXX%20-%20PAR64%20COB%20RGB%20100W.pdf"
    ],
    "productPage": [
      "https://www.musicstore.com/en_OE/EUR/lightmaXX-LED-PAR-64-COB-100W-RGB-polish/art-LIG0010192-000"
    ]
  },
  "physical": {
    "dimensions": [240, 195, 390],
    "weight": 2.6,
    "power": 120,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "LED Array Cluster"
    },
    "lens": {
      "degreesMinMax": [30, 30]
    }
  },
  "availableChannels": {
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Dimmer": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Special": {
      "capabilities": [
        {
          "dmxRange": [0, 0],
          "type": "NoFunction"
        },
        {
          "dmxRange": [1, 5],
          "type": "Generic",
          "comment": "Sound Control (7 Colors)"
        },
        {
          "dmxRange": [6, 10],
          "type": "Generic",
          "comment": "ON"
        },
        {
          "dmxRange": [11, 255],
          "type": "StrobeSpeed",
          "speedStart": "stop",
          "speedEnd": "fast",
          "comment": "Strobe (increasing)"
        }
      ]
    "Sound / Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 0],
          "type": "NoFunction",
          "helpWanted": "Is the light on or off in this DMX range?"
        },
        {
          "dmxRange": [1, 5],
          "type": "Effect",
          "effectName": "Sound Control (7 Colors)",
          "soundControlled": true
        },
        {
          "dmxRange": [6, 10],
          "type": "ShutterStrobe",
          "strobeEffect": "Open"
        },
        {
          "dmxRange": [11, 255],
          "type": "ShutterStrobe",
          "strobeEffect": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    }
  },
  "modes": [
    {
      "name": "3-channel",
      "shortName": "3ch",
      "channels": [
        "Red",
        "Green",
        "Blue"
      ]
    },
    {
      "name": "5-channel",
      "shortName": "5ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Dimmer",
        "Sound / Strobe"
      ]
    }
