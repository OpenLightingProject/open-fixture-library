{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "CLS Nano COB",
  "categories": ["Stand", "Color Changer", "Dimmer", "Effect"],
  "meta": {
    "authors": ["prtr", "Luke Nelson"],
    "createDate": "2023-10-15",
    "lastModifyDate": "2023-10-15"
  },
  "comment": "4 RGB COB Spots, 30W each",
  "links": {
    "manual": [
      "https://www.musicstore.com/INTERSHOP/web/WFS/MusicStore-MusicStoreShop-Site/de_DE/ms-ch/CHF/ViewProductAttachment-ResolveManualURL?File=DE_LIG0015657-000.pdf"
    ],
    "productPage": [
      "https://www.musicstore.com/en_OE/EUR/lightmaXX-CLS-Nano-COB/art-LIG0015657-000"
    ],
    "video": [
      "https://www.youtube.com/watch?v=_Jfp0JCAOns"
    ]
  },
  "physical": {
    "dimensions": [720, 200, 110],
    "weight": 5.7,
    "power": 130,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "LED"
    },
    "lens": {
      "degreesMinMax": [25, 25]
    }
  },
  "matrix": {
    "pixelCount": [4, 1, 1],
    "pixelGroups": {
      "All": "all"
    }
  },
  "templateChannels": {
    "Red $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue $pixelKey": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    }
  },
  "availableChannels": {
    "Dimmer": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Strobe": {
      "capability": {
        "type": "ShutterStrobe",
        "shutterEffect": "Strobe",
        "speedStart": "slow",
        "speedEnd": "fast",
        "helpWanted": "At which DMX values is strobing disabled?"
      }
    },
    "Color Presets": {
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 99],
          "type": "Intensity",
          "brightness": "100%",
          "comment": "Full ON"
        },
        {
          "dmxRange": [100, 129],
          "type": "Effect",
          "effectPreset": "ColorJump",
          "comment": "erratic"
        },
        {
          "dmxRange": [130, 159],
          "type": "Effect",
          "effectPreset": "ColorFade"
        },
        {
          "dmxRange": [160, 189],
          "type": "Effect",
          "effectPreset": "ColorJump"
        },
        {
          "dmxRange": [190, 219],
          "type": "Effect",
          "soundControlled": true,
          "effectName": "Music Mode 1"
        },
        {
          "dmxRange": [220, 249],
          "type": "Effect",
          "soundControlled": true,
          "effectName": "Music Mode 2"
        },
        {
          "dmxRange": [250, 255],
          "type": "Effect",
          "soundControlled": true,
          "effectName": "Music Mode 3"
        }
      ]
    },
    "Program Speed": {
      "capability": {
        "type": "EffectSpeed",
        "speedStart": "slow",
        "speedEnd": "fast"
      }
    }
  },
  "modes": [
    {
      "name": "4-channel",
      "shortName": "4ch",
      "channels": [
        "Dimmer",
        "Red All",
        "Green All",
        "Blue All"
      ]
    },
    {
      "name": "7-channel",
      "shortName": "7ch",
      "channels": [
        "Dimmer",
        "Strobe",
        "Red All",
        "Green All",
        "Blue All",
        "Color Presets",
        "Program Speed"
      ]
    },
    {
      "name": "12-channel",
      "shortName": "12ch",
      "channels": [
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelXYZ",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        }
      ]
    },
    {
      "name": "16ch individual Spot RGB + effects",
      "channels": [
        "Dimmer",
        "Strobe",
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelXYZ",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        },
        "Color Presets",
        "Program Speed"
      ]
    }
  ]
}
