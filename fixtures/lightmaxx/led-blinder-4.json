{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/schema-12.4.0/schemas/fixture.json",
  "name": "LED Blinder 4",
  "categories": ["Matrix", "Strobe", "Blinder"],
  "meta": {
    "authors": ["Leon Dietrich"],
    "createDate": "2026-02-02",
    "lastModifyDate": "2026-02-02"
  },
  "links": {
    "productPage": [
      "https://www.musicstore.com/en_ES/EUR/lightmaXX-LED-Blinder-4/art-LIG0016527-000"
    ],
    "video": [
      "https://www.youtube.com/watch?v=QpehbQvLv7c&pp=ygUXbGlnaHRtYXh4IGxlZCBibGluZGVyIDQ%3D"
    ]
  },
  "physical": {
    "dimensions": [195, 315, 315],
    "weight": 4.5,
    "power": 210,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "LED"
    }
  },
  "matrix": {
    "pixelCount": [2, 2, 1]
  },
  "templateChannels": {
    "Intensity $pixelKey": {
      "capability": {
        "type": "Intensity"
      }
    }
  },
  "availableChannels": {
    "Dimmer": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 0],
          "type": "ShutterStrobe",
          "shutterSpeed": "Open",
          "comment": "No Strobe Effect"
        },
        {
          "dmxRange": [1, 255],
          "type": "StrobeSpeed",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    },
    "Effect": {
      "capabilities": [
        {
          "dmxRange": [0, 70],
          "type": "Effect",
          "effectName": "on-off-iterate",
          "speedStart": "slow",
          "speedEnd": "fast"
        },
        {
          "dmxRange": [71, 140],
          "type": "Effect",
          "effectName": "fade-iterate",
          "speedStart": "slow",
          "speedEnd": "fast"
        },
        {
          "dmxRange": [141, 200],
          "type": "Effect",
          "effectName": "fade rotated",
          "speedStart": "slow",
          "speedEnd": "fast"
        },
        {
          "dmxRange": [201, 255],
          "type": "Effect",
          "effectName": "on-off rotated",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    },
    "No function": {
      "capability": {
        "type": "NoFunction"
      }
    }
  },
  "modes": [
    {
      "name": "8ch",
      "channels": [
        "Dimmer",
        "Strobe Speed",
        "Effect",
        "No function",
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelXYZ",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Intensity $pixelKey"
          ]
        }
      ]
    }
  ]
}
