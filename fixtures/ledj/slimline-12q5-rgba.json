{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "Slimline 12Q5 RGBA",
  "shortName": "LEDJ67B",
  "categories": ["Pixel Bar", "Color Changer"],
  "meta": {
    "authors": ["Felix Edelmann"],
    "createDate": "2019-03-14",
    "lastModifyDate": "2019-03-14"
  },
  "links": {
    "manual": [
      "https://prolight.co.uk/ftp-in/LEDJ67B_Manual.pdf"
    ],
    "productPage": [
      "https://prolight.co.uk/product/slimline-12q5-rgba-batten-black-housing",
      "https://prolight.co.uk/product/slimline-12q5-rgba-batten-white-housing"
    ],
    "video": [
      "https://www.youtube.com/watch?v=q9vZgmnNnco"
    ]
  },
  "physical": {
    "dimensions": [87, 1018, 63],
    "weight": 2.2,
    "power": 70,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "12x 5W RGBA LEDs"
    },
    "lens": {
      "degreesMinMax": [25, 25]
    }
  },
  "matrix": {
    "pixelCount": [12, 1, 1],
    "pixelGroups": {
      "Master": "all"
    }
  },
  "availableChannels": {
    "Dimmer / Strobe": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 240],
          "type": "Intensity"
        },
        {
          "dmxRange": [241, 255],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "helpWanted": "Is the strobe speed controllable here (e.g. slowâ€¦fast)?"
        }
      ]
    },
    "Dimmer": {
      "defaultValue": 0,
      "capability": {
        "type": "Intensity"
      }
    },
    "Strobe": {
      "defaultValue": 0,
      "capability": {
        "type": "ShutterStrobe",
        "shutterEffect": "Strobe",
        "speedStart": "slow",
        "speedEnd": "fast",
        "helpWanted": "At which DMX value is strobe disabled? When is the lamp constantly on/off?"
      }
    },
    "Color Macros": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 4],
          "type": "NoFunction"
        },
        {
          "dmxRange": [5, 80],
          "type": "ColorPreset",
          "comment": "Color macro",
          "helpWanted": "Which colors can be selected at which DMX values? Or is this a color change effect?"
        },
        {
          "dmxRange": [81, 150],
          "type": "Effect",
          "effectPreset": "ColorJump"
        },
        {
          "dmxRange": [151, 255],
          "type": "Effect",
          "effectPreset": "ColorFade"
        }
      ]
    }
  },
  "templateChannels": {
    "Red $pixelKey": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green $pixelKey": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue $pixelKey": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Amber $pixelKey": {
      "defaultValue": 0,
      "capability": {
        "type": "ColorIntensity",
        "color": "Amber"
      }
    }
  },
  "modes": [
    {
      "name": "2-channel",
      "shortName": "2ch",
      "channels": [
        "Dimmer / Strobe",
        "Color Macros"
      ]
    },
    {
      "name": "3-channel",
      "shortName": "3ch",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master"
      ]
    },
    {
      "name": "4-channel",
      "shortName": "4ch",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master",
        "Amber Master"
      ]
    },
    {
      "name": "7-channel",
      "shortName": "7ch",
      "channels": [
        "Dimmer",
        "Strobe",
        "Red Master",
        "Green Master",
        "Blue Master",
        "Amber Master",
        "Color Macros"
      ]
    }
  ]
}
