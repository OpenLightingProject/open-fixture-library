{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "Precision DMX",
  "categories": [
    "Color Changer"
  ],
  "meta": {
    "authors": [
      "Brian McDade"
    ],
    "createDate": "2020-03-28",
    "lastModifyDate": "2020-03-28"
  },
  "links": {
    "manual": [
      "https://www.gantom.com/wp-content/uploads/2018/07/Precision-DMX-User-Guide-1.1.pdf",
      "https://www.gantom.com/wp-content/uploads/2018/05/Precision_DMX_2018.pdf"
    ],
    "video": [
      "https://www.youtube.com/watch?v=Oi_wBNiZeQk"
    ]
  },
  "helpWanted": "Does the 3ch mode really exist? It is never mentioned in the manual.",
  "physical": {
    "dimensions": [
      44.5,
      52.5,
      52
    ],
    "power": 3.8,
    "DMXconnector": "3.5mm stereo jack",
    "bulb": {
      "type": "RGB LED"
    },
    "lens": {
      "degreesMinMax": [
        21,
        21
      ]
    }
  },
  "availableChannels": {
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Color Macros": {
      "capabilities": [
        {
          "dmxRange": [
            0,
            31
          ],
          "type": "NoFunction"
        },
        {
          "dmxRange": [
            32,
            255
          ],
          "type": "ColorPreset",
          "helpWanted": "At which DMX value can which color be selected?"
        }
      ]
    },
    "Strobe": {
      "capability": {
        "type": "ShutterStrobe",
        "shutterEffect": "Strobe",
        "speedStart": "slow",
        "speedEnd": "fast",
        "helpWanted": "At which DMX values is strobe disabled? When is the lamp constantly on/off?"
      }
    },
    "Effect Speed": {
      "capability": {
        "type": "EffectSpeed",
        "speedStart": "slow",
        "speedEnd": "fast"
      }
    },
    "Built-in Functions": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [
            0,
            0
          ],
          "type": "NoFunction",
          "switchChannels": {
            "Strobe / Effect Speed": "Strobe"
          }
        },
        {
          "dmxRange": [
            1,
            31
          ],
          "type": "NoFunction",
          "helpWanted": "What does this capability do? Is channel 5 really set to 'Effect Speed', and which speed is controlled then?",
          "switchChannels": {
            "Strobe / Effect Speed": "Effect Speed"
          }
        },
        {
          "dmxRange": [
            32,
            71
          ],
          "type": "ShutterStrobe",
          "shutterEffect": "RampUp",
          "switchChannels": {
            "Strobe / Effect Speed": "Effect Speed"
          }
        },
        {
          "dmxRange": [
            72,
            111
          ],
          "type": "ShutterStrobe",
          "shutterEffect": "RampDown",
          "switchChannels": {
            "Strobe / Effect Speed": "Effect Speed"
          }
        },
        {
          "dmxRange": [
            112,
            151
          ],
          "type": "ShutterStrobe",
          "shutterEffect": "Pulse",
          "switchChannels": {
            "Strobe / Effect Speed": "Effect Speed"
          }
        },
        {
          "dmxRange": [
            152,
            191
          ],
          "type": "Effect",
          "effectPreset": "ColorFade",
          "switchChannels": {
            "Strobe / Effect Speed": "Effect Speed"
          }
        },
        {
          "dmxRange": [
            192,
            231
          ],
          "type": "Effect",
          "effectPreset": "ColorJump",
          "comment": "red, green, blue",
          "switchChannels": {
            "Strobe / Effect Speed": "Effect Speed"
          }
        },
        {
          "dmxRange": [
            232,
            250
          ],
          "type": "Effect",
          "effectPreset": "ColorJump",
          "comment": "6-color",
          "switchChannels": {
            "Strobe / Effect Speed": "Effect Speed"
          }
        },
        {
          "dmxRange": [
            251,
            255
          ],
          "type": "Effect",
          "effectName": "Candle Flicker",
          "switchChannels": {
            "Strobe / Effect Speed": "Effect Speed"
          }
        }
      ]
    },
    "Master Dimmer": {
      "capability": {
        "type": "Intensity"
      }
    }
  },
  "modes": [
    {
      "name": "3-channel",
      "shortName": "3ch",
      "channels": [
        "Red",
        "Green",
        "Blue"
      ]
    },
    {
      "name": "7-channel",
      "shortName": "7ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Color Macros",
        "Strobe / Effect Speed",
        "Built-in Functions",
        "Master Dimmer"
      ]
    }
  ]
}
